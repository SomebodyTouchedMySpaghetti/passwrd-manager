{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Key managment pre-alpha",
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["./dist/main.js"]
    }
  ],
  "browser_action":{
    "default_icon":{
      "19":"icon-19.png",
      "38":"icon-38.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "48": "icon-48.png",
    "96": "icon-96.png"
  },
  "background":{
    "scripts":["background.js"]
  },
  "permissions":[
    "tabs"
  ]
}